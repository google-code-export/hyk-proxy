<?xml version="1.0" encoding="utf-8"?>
<Configure>
	<!-- Can config more than 1 hyk-proxy-server -->
	<GAE>
	   <Server appid="hykserver" />
	</GAE>
	<!-- <hyk-proxy-server appid="" user="" passwd=""/> -->

	<!-- HTTP/HTTPS/XMPP-->
	<ConnectionMode>HTTP</ConnectionMode>

	<!-- Can config more than 1 account -->
	<XMPP>
	   <Account user="" passwd="" />
	   	<!-- <Account user="" passwd="" serverHost="" serverPort="" oldSSLEnable="" 
		/> -->
	</XMPP>

	<!-- Warning:Don't modify the configurations below if you don't know the 
		meaning of them -->
	<!-- Advanced Options -->
	<!-- local server would wait so many secs for every http request -->
	<SessionTimeOut>60</SessionTimeOut>
	<!--Avoid some edge case for full url path -->
	<!--This option is not active when through a unanonymouse local proxy -->
	<SimpleURLEnable>true</SimpleURLEnable>
	<!--Can choose from LZF/Snappy/FastLZ/QuickLZ/None -->
	<Compressor>Snappy</Compressor>
	<!--Can choose from SE1/None -->
	<Encrypter>SE1</Encrypter>
	<ConnectionPoolSize>7</ConnectionPoolSize>
	<FetchLimitSize>250000</FetchLimitSize>
	<!-- for big file(>=1m) & video -->
	<ConcurrentRangeFetchWorker>3</ConcurrentRangeFetchWorker>

	<!-- If you want use hyk-proxy througth a proxy, config this item -->
	<LocalProxy type="HTTP" host="localhost" port="8888"/>
	<GoogleProxyChain enable="false" host="www.google.com.hk"/>

	<AppIdBindings>
		<!--
		   <Binding appid="hyk-proxy-demo">
		        <site>twitter</site> 
		         <site>youtube</site> 
		   </Binding> 
		-->
	</AppIdBindings>

	<!-- Sometimes, you already know you can inject a 'Range' header in the 
		proxy request to avoid a fail-try request. Use ';' as the separator for sites -->
	<InjectRangeHeaderSites>.youtube.com</InjectRangeHeaderSites>
	

	<UserAgent>
      <![CDATA[
          Mozilla/5.0 (Windows; U; Windows NT 5.1; en-US) AppleWebKit/532.5 (KHTML, like Gecko) Chrome/4.1.249.1045 Safari/532.5
      ]]>
	</UserAgent>
	
	<MappingHosts>
	    <Mapping src="" dst=""/>
	</MappingHosts>
</Configure>